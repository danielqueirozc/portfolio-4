generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model technology {
  id          Int     @id @default(autoincrement())
  name        String
  description String
  image       String
  emphasis    Boolean @default(false)

  @@map("technologies")
}
